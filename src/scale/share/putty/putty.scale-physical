Protocol=serial
LocalEcho=0
LocalEdit=1
LFImpliesCR=0
CRImpliesLF=1
TermWidth=80
TermHeight=24
SerialLine=/dev/scale-board
SerialSpeed=9600
SerialDataBits=8
SerialStopHalfbits=2
SerialParity=0
SerialFlowControl=0
